neuron CoCoIncorrectReturnStatement:
    update:
        # required for creation of a correct ast
    end

    input:
        # required for creation of a correct ast
    end

    output: spike

    function foo() mV:# correct return type is given
        test_building_symboltable_for_all_neurons mV = 10mV

        if test_building_symboltable_for_all_neurons == 10mV:
            return test_building_symboltable_for_all_neurons
        else:
            return 20mV
        end
        return 5mV
    end

    function noReturn():# correct return type is given
        return
    end

    function bar() mV:# correct return type is given
        return 5mV
    end

    function foobar() mV:# correct return type is given
        test_building_symboltable_for_all_neurons mV = 10mV
        test_building_symboltable_for_all_neurons = test_building_symboltable_for_all_neurons*2
        return test_building_symboltable_for_all_neurons
    end

end